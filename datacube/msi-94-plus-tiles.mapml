<mapml- xmlns="http://www.w3.org/1999/xhtml">
  <map-head>
    <map-title>Permanent snow and ice (prob. 94% or more) WMS GetMap - Tiled WMS GetMap</map-title>
    <map-meta charset="utf-8"></map-meta>
    <map-link href="https://datacube.services.geo.ca/ows/msi?SERVICE=WMS&amp;REQUEST=GetCapabilities&amp;VERSION=1.3.0" rel="license" title="Open Government License"/>
    <map-link rel="legend" href="https://datacube.services.geo.ca/assets/legend/legend_msi_binary.png" sizes="150x33" height="33" width="150" title="legend" type="image/png" />
    <map-link rel="style" href="msi-94-plus.mapml" title="WMS GetMap" />
    <map-link rel="self style" href="msi-94-plus-tiles.mapml" title="Tiled WMS GetMap" />
  </map-head>
  <map-body>
    <map-extent units="CBMTILE" checked="checked">
      <map-input name="z" type="zoom" value="25" min="4" max="25"/>
      <map-input name="txmin" type="location" rel="tile" position="top-left" axis="easting" units="tilematrix" min="-6105785.728719063" max="6174630.753072981"/>
      <map-input name="tymin" type="location" rel="tile" position="bottom-left" axis="northing" units="tilematrix" min="-1692155.9568245453" max="4482681.2054050295"/>
      <map-input name="txmax" type="location" rel="tile" position="top-right" axis="easting" units="tilematrix" min="-6105785.728719063" max="6174630.753072981"/>
      <map-input name="tymax" type="location" rel="tile" position="top-left" axis="northing" units="tilematrix" min="-1692155.9568245453" max="4482681.2054050295"/>
      <map-select id="tie" name="time">
        <map-option>2000-08-15T00:00:00Z</map-option>
        <map-option>2001-08-15T00:00:00Z</map-option>
        <map-option>2002-08-15T00:00:00Z</map-option>
        <map-option>2003-08-15T00:00:00Z</map-option>
        <map-option>2004-08-15T00:00:00Z</map-option>
        <map-option>2005-08-15T00:00:00Z</map-option>
        <map-option>2006-08-15T00:00:00Z</map-option>
        <map-option>2007-08-15T00:00:00Z</map-option>
        <map-option>2008-08-15T00:00:00Z</map-option>
        <map-option>2009-08-15T00:00:00Z</map-option>
        <map-option>2010-08-15T00:00:00Z</map-option>
        <map-option>2011-08-15T00:00:00Z</map-option>
        <map-option>2012-08-15T00:00:00Z</map-option>
        <map-option>2013-08-15T00:00:00Z</map-option>
        <map-option>2014-08-15T00:00:00Z</map-option>
        <map-option>2015-08-15T00:00:00Z</map-option>
        <map-option>2016-08-15T00:00:00Z</map-option>
        <map-option>2017-08-15T00:00:00Z</map-option>
        <map-option>2018-08-15T00:00:00Z</map-option>
        <map-option>2019-08-15T00:00:00Z</map-option>
        <map-option>2020-08-15T00:00:00Z</map-option>        
      </map-select>
      <map-link tref="https://datacube.services.geo.ca/ows/msi?SERVICE=WMS&amp;REQUEST=GetMap&amp;FORMAT=image/png&amp;TRANSPARENT=TRUE&amp;STYLES=msi-binary&amp;VERSION=1.3.0&amp;LAYERS=msi-94-or-more&amp;WIDTH=256&amp;HEIGHT=256&amp;CRS=EPSG:3978&amp;BBOX={txmin},{tymin},{txmax},{tymax}&amp;TIME={time}" rel="tile"/>
      <map-link tref="legend_query_msi-94-plus.mapml?BBOX={txmin},{tymin},{txmax},{tymax}&amp;TIME={time}" rel="query"/>
    </map-extent>
  </map-body>
</mapml->