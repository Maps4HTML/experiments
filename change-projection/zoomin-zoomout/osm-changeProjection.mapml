<mapml- xmlns="http://www.w3.org/1999/xhtml">
  <map-head>
    <map-meta charset="utf-8"/>
    <map-title>OpenStreetMap</map-title>
    <map-link rel="license" href="https://www.openstreetmap.org/copyright" title="Â© OpenStreetMap contributors CC BY-SA"/>
    <map-link rel="zoomout" href="cbmt-changeProjection.mapml" type="text/mapml" />
  </map-head>
  <map-body>
    <map-extent units="OSMTILE">
      <map-input name="z" type="zoom" min="9" max="18"/>
      <map-input name="s" type="hidden" shard="true" list="servers"/>
      <map-datalist id="servers">
        <map-option value="a"/>
        <map-option value="b"/>
        <map-option value="c"/>
      </map-datalist>
      <map-input name="x" type="location" units="tilematrix" axis="column"/>
      <map-input name="y" type="location" units="tilematrix" axis="row"/>
      <map-link rel="tile" tref="https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png"/>
    </map-extent>
  </map-body>
</mapml->
